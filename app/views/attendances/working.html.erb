<% provide(:title, '出勤中') %>
<h1>出勤中社員一覧</h1>


<div>
  <table class = "table-bordered table-condensed">
   <thead>  
    <tr>
      <th>社員番号</th>
      <th>名前</th>
    </tr>
   </thead>
   
   <tbody>

    <% @users.each do |user| %>
     <tr>
       <th></th>
       <th><%= user.name %></th>
     </tr>
   <% end %>
   </tbody>
   
  </table>
</div>  